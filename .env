APP_PORT=8081

# JWT secret used for signing HS256 tokens. Change this value in production.
JWT_SECRET=dev-super-secret-change-me

# Database configuration
# Option 1: Set DATABASE_URL directly (uncomment and adjust if you prefer)
# DATABASE_URL=postgres://postgres:postgres@localhost:5432/pencatatan_data_mahasiswa?sslmode=disable

# Option 2: Use split variables (the app will build DATABASE_URL from these)
DATABASE_HOST=127.0.0.1
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=12345678
DATABASE_NAME=db_mahasiswa
DATABASE_SSLMODE=disable